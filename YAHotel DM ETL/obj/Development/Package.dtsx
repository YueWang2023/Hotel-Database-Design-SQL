                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
YAHotel.dbo.Orders.OrderID AS OrderID,
YAHotel.dbo.Orders.Employee AS Employee_SK,
YAHotel.dbo.Orders.Customer_SK AS Customer_SK,
YAHotel.dbo.Orders.Room_SK AS Room_SK,
YAHotel.dbo.Orders.ReservationPrice AS ReservationPrice,
YAHotel.dbo.Orders.ReservationDate AS ReservationDate,
