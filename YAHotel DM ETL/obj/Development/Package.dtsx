YAHotel.dbo.Orders.EndDate AS EndDate,
YAHotel.dbo.Orders.StayTime AS StayTime,
YAHotel.dbo.Orders.NumberofPeople AS NumberofPeople

FROM 

YAHotel.dbo.Orders

INNER JOIN 
	YAHotel.dbo.Employees
ON 
	YAHotel.dbo.Employees.EmployeeID = YAHotel.dbo.Orders.Employee

INNER JOIN 
	YAHotel.dbo.Customers
ON 
	YAHotel.dbo.Customers.CustomerID = YAHotel.dbo.Orders.Customer_SK

INNER JOIN 
	YAHotel.dbo.Rooms
ON 
	YAHotel.dbo.Rooms.RoomID = YAHotel.dbo.Orders.Room_SK

INNER JOIN 
	YAHotel.dbo.DimDate
ON 
	YAHotel.dbo.DimDate.Date_SK = YAHotel.dbo.Orders.StartDate</property>
